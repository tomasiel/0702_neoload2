<scenario postMonitoringTime="-1" preMonitoringTime="-1"
    slaProfileEnabled="false" traceVariables="true" uid="scenario1" virtualUsersStates="true">
    <!--****** POPULATION POLICY ******-->
    <population-policy name="Population1">
        <duration-policy-entry iterations="1" time="120" timeUnit="0" type="2"/>
        <volume-policy-entry>
            <rampup-volume-policy delayIncrement="2.0"
                delayTypeIncrement="2" initialUserNumber="2"
                iterationNumber="1" maxUserNumber="0" userIncrement="2"/>
        </volume-policy-entry>
        <start-stop-policy-entry start-delay="0" start-type="1"
            stop-delay="60000" stop-type="0"/>
        <runtime-policy continueOnError="true" thinktimePolicy="0"
            thinktimeValue="5000" vuStartDelay="10000" vuStartMode="0"/>
        <!--****** POPULATION LG HOSTS ******-->
        <lg-hosts>
            <lg-host-entry>$zoneID=Default zone;$lgID=localhost:7100</lg-host-entry>
        </lg-hosts>
    </population-policy>
    <!--****** POPULATION POLICY ******-->
    <population-policy name="s">
        <duration-policy-entry iterations="1" time="120" timeUnit="0" type="2"/>
        <volume-policy-entry>
            <constant-volume-policy iterationNumber="1" userNumber="10"/>
        </volume-policy-entry>
        <start-stop-policy-entry start-delay="0" start-type="0"
            stop-delay="60000" stop-type="0"/>
        <runtime-policy continueOnError="true" thinktimePolicy="0"
            thinktimeValue="5000" vuStartDelay="0" vuStartMode="0"/>
        <!--****** POPULATION LG HOSTS ******-->
        <lg-hosts>
            <lg-host-entry>$zoneID=Default zone;$lgID=localhost:7100</lg-host-entry>
        </lg-hosts>
    </population-policy>
    <dashboards defaultDashboardUid="Graph board"/>
    <!--****** DASHBOARDS  ******-->
    <charts-model-description name="Graph board">
        <chart-description autoRange="true"
            chart-description-type="chart-description-type"
            domainMarkerVisible="true" granularityLevel="0"
            historyState="XMIN" includeInReport="false"
            rangeMarkerVisible="true"
            title="SignonPg - Minimum duration" typeName="CLASSIC">
            <action-curve-description
                actionUid="2d5ef1b8-faf8-4b2e-a27c-62e4370e8063"
                color="218,218,0" dashed="false" populationName=""
                scale="1.0" selected="false"
                statisticName="result.minimum.duration" zoneName=""/>
            <action-curve-description
                actionUid="56b2401d-4a0f-45b9-8601-7294e54dca84"
                color="153,153,0" dashed="false" populationName=""
                scale="1.0" selected="false"
                statisticName="result.average.duration"
                virtualUserUid="MyInfoFlow" zoneName=""/>
            <action-curve-description actionUid="MyInfoFlow"
                color="20,106,203" dashed="false" populationName=""
                scale="1.0" selected="true"
                statisticName="result.average.duration"
                virtualUserUid="MyInfoFlow" zoneName=""/>
        </chart-description>
        <chart-description autoRange="true"
            chart-description-type="chart-description-type"
            domainMarkerVisible="true" granularityLevel="0"
            historyState="XMIN" includeInReport="false"
            rangeMarkerVisible="true"
            title="SignonPg - Average duration" typeName="CLASSIC">
            <action-curve-description
                actionUid="2d5ef1b8-faf8-4b2e-a27c-62e4370e8063"
                color="153,153,0" dashed="false" populationName=""
                scale="1.0" selected="true"
                statisticName="result.average.duration"
                virtualUserUid="MyInfoFlow" zoneName=""/>
        </chart-description>
        <chart-description autoRange="true"
            chart-description-type="chart-description-type"
            domainMarkerVisible="true" granularityLevel="0"
            historyState="XMIN" includeInReport="false"
            rangeMarkerVisible="true"
            title="/web/index.php/auth/validate - Average duration" typeName="CLASSIC">
            <action-curve-description
                actionUid="8ed368aa-9249-42d5-a251-07ed8de6015a"
                color="153,153,0" dashed="false" populationName=""
                scale="1.0" selected="true"
                statisticName="result.average.duration"
                virtualUserUid="MyInfoFlow" zoneName=""/>
        </chart-description>
        <chart-description autoRange="true"
            chart-description-type="chart-description-type"
            description="If the CPU usage regularly exceeds 95%, more Load Generators should be added."
            domainMarkerVisible="true" granularityLevel="0"
            historyState="XMIN" includeInReport="false"
            rangeMarkerVisible="true" title="CPU usage per LG" typeName="CLASSIC">
            <monitor-curve-description color="20,106,203"
                connectionName="NeoLoad" dashed="false"
                monitorName="UserLoad" monitoredHostName="NeoLoad"
                scale="1.0" selected="false"/>
            <monitor-curve-description color="0,255,0"
                connectionName="NeoLoad" dashed="false"
                monitorName="lgcpu.localhost:7100"
                monitoredHostName="LG localhost:7100" scale="1.0" selected="false"/>
        </chart-description>
        <chart-description autoRange="true"
            chart-description-type="chart-description-type"
            domainMarkerVisible="true" granularityLevel="0"
            historyState="XMIN" includeInReport="false"
            rangeMarkerVisible="true"
